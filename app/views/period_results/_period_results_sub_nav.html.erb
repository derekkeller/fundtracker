<div id="period_results_sub_nav_years">
	<ul>
		<% for year in @years %>
			<li><%= year %></li>
		<% end %>
	</ul>
</div>
<div id="period_results_sub_nav_type">
	<ul>
		<li class="<%= check_view_type(session[:view_type], 'sales') %>">
			<%= link_to('Sales %', period_result_change_view_type_path(5, :company_id => @company.id), :method => 'put') %>
		</li>
		<li class="<%= check_view_type(session[:view_type], 'growth') %>">
			<%= link_to('Growth', period_result_change_view_type_path(4, :company_id => @company.id), :method => 'put') %>
		</li>
		<li class="<%= check_view_type(session[:view_type], 'variance') %>">
			<%= link_to('Variance', period_result_change_view_type_path(3, :company_id => @company.id), :method => 'put') %>
		</li>					
		<li class="<%= check_view_type(session[:view_type], 'forecast') %>">
			<%= link_to('Forecast', period_result_change_view_type_path(2, :company_id => @company.id), :method => 'put') %>
		</li>
		<li class="<%= check_view_type(session[:view_type], 'actual') %>">
			<%= link_to('Actual', period_result_change_view_type_path(1, :company_id => @company.id), :method => 'put') %>
		</li>
	</ul>
</div>
<hr class="clear"/>
<div id="period_results_sub_nav_view_by">
	<ul>
		<li class="<%= check_view_by(session[:view_by], 'year') %>">
			<%= link_to 'Year', period_result_change_period_path(1, :company_id => @company.id), :method => 'put' %>
		</li>
		<li class="<%= check_view_by(session[:view_by], 'quarter') %>">
			<%= link_to 'Quarter', period_result_change_period_path(2, :company_id => @company.id), :method => 'put' %>
		</li>
		<li class="<%= check_view_by(session[:view_by], 'month') %>">
			<%= link_to 'Month', period_result_change_period_path(3, :company_id => @company.id), :method => 'put' %>
		</li>
		<li>View by:</li>
	</ul>
</div>
<hr class="clear"/>
